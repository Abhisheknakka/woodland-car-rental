# Admin Portal Environment Variables
# Copy this file to .env.local and fill in your values

# API Configuration
VITE_API_URL=http://localhost:3001/api

# Payment Gateway Keys (Public keys only)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
VITE_RAZORPAY_KEY_ID=rzp_test_your_razorpay_key_id

# App Configuration
VITE_APP_NAME=Woodland Car Rental Admin
VITE_APP_URL=http://localhost:3002
VITE_COMPANY_NAME=Woodland Car Rental
VITE_COMPANY_EMAIL=admin@woodlandcarrental.com

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_EXPORT=true
VITE_ENABLE_BULK_OPERATIONS=true

# Development Settings
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info
