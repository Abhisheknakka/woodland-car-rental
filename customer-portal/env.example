# Customer Portal Environment Variables
# Copy this file to .env.local and fill in your values

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3001/api

# Payment Gateway Keys (Development)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key_here
NEXT_PUBLIC_RAZORPAY_KEY_ID=rzp_test_your_razorpay_key_here

# Google Maps API (Optional)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Analytics (Optional)
NEXT_PUBLIC_GA_TRACKING_ID=your_ga_tracking_id_here

# Feature Flags
NEXT_PUBLIC_ENABLE_PAYMENTS=true
NEXT_PUBLIC_ENABLE_REVIEWS=true
NEXT_PUBLIC_ENABLE_NOTIFICATIONS=true

# App Configuration
NEXT_PUBLIC_APP_NAME=Woodland Car Rental
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_CONTACT_PHONE=+91 98765 43210
NEXT_PUBLIC_CONTACT_EMAIL=info@woodlandcarrental.com
NEXT_PUBLIC_CONTACT_ADDRESS=123 Main Street, City, State 12345
